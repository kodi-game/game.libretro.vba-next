msgid "Use BIOS if available (Restart)"
msgstr "使用 BIOS 如可用（重新启动）"

msgctxt "#30002"
msgid "Force Enable RTC (Restart)"
msgstr "强制启用 RTC（重新启动）"

msgctxt "#30003"
msgid "Enable Turbo Buttons"
msgstr "启用 Turbo 按钮"

msgctxt "#30004"
msgid "Turbo Delay in frames"
msgstr "帧中的 Turbo 延迟"
